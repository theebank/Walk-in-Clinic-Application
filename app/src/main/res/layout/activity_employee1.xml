<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Employee1">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textView18"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:text="Welcome to the Employee Homepage." />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <Button
                android:id="@+id/UpdateBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="accountupdatebtn"
                android:text="Complete your profile" />

            <Button
                android:id="@+id/ServiceBtn"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="servicebtn"
                android:text="Service Options"
                tools:visibility="invisible" />

            <Button
                android:id="@+id/Clinichrs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="clinichrs"
                android:text="Manage Clinic Hours"
                tools:visibility="invisible" />

            <Button
                android:id="@+id/Managehrs"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:onClick="managehrs"
                android:text="Manage your hours"
                tools:visibility="invisible" />
        </LinearLayout>
    </LinearLayout>
</androidx.constraintlayout.widget.ConstraintLayout>